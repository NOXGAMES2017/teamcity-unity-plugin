<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

    <import resource="classpath*:META-INF/build-common-plugin-unity.xml"/>

    <bean class="jetbrains.buildServer.unity.UnityBuildFeature"/>
    <bean class="jetbrains.buildServer.unity.UnityPasswordsProvider"/>
    <bean class="jetbrains.buildServer.unity.UnityRunnerRunType"/>
    <bean class="jetbrains.buildServer.unity.UnityRunnerDiscoveryExtension"/>
    <bean class="jetbrains.buildServer.unity.UnityServerToolProvider"/>
    <bean class="jetbrains.buildServer.unity.UnityToolType"/>
    <bean class="jetbrains.buildServer.unity.fetchers.UnityBuildTargetFetcher"/>
    <bean class="jetbrains.buildServer.unity.fetchers.UnityStaticMethodFetcher"/>
</beans>